{
  "denhaag": {
    "accordion": {
      "font-family": {
        "value": "{denhaag.typography.sans-serif.font-family}"
      },
      "margin-block-end": { "value": "{denhaag.space.block.xl}" },
      "section": {
        "border": {
          "value": "1px solid {denhaag.color.grey.3}"
        },
        "content": {
          "background-color": { "value": "{denhaag.color.white}" },
          "padding-block-end": { "value": "24px" },
          "padding-block-start": { "value": "24px" },
          "padding-inline-end": { "value": "16px" },
          "padding-inline-start": { "value": "16px" }
        },
        "header": {
          "background-color": { "value": "{denhaag.color.white}" },
          "column-gap": { "value": "8px" },
          "cursor": { "value": "pointer" },
          "font-size": { "value": "18px" },
          "line-height": { "value": "24px" },
          "padding-block-end": { "value": "12px" },
          "padding-block-start": { "value": "12px" },
          "padding-inline-end": { "value": "16px" },
          "padding-inline-start": { "value": "16px" },
          "icon": {
            "transform": { "value": "rotate(0deg)" },
            "transition": { "value": "transform 400ms ease-in-out" },
            "expanded": {
              "transform": { "value": "rotate(180deg)" }
            }
          }
        },
        "title": {
          "font-size": { "value": "inherit" }
        }
      }
    }
  }
}
